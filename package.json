{
  "name": "english-tree-monorepo",
  "version": "1.0.0",
  "description": "大树英语学习应用 - Monorepo",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "dev:frontend": "cd apps/frontend && pnpm start",
    "dev:backend": "cd apps/backend && pnpm run start:dev",
    "build": "pnpm run build:backend && pnpm run build:frontend",
    "build:frontend": "cd apps/frontend && pnpm run build",
    "build:backend": "cd apps/backend && pnpm run build",
    "test": "pnpm run test:backend",
    "test:backend": "cd apps/backend && pnpm test",
    "lint": "pnpm run lint:frontend && pnpm run lint:backend",
    "lint:frontend": "cd apps/frontend && pnpm run lint",
    "lint:backend": "cd apps/backend && pnpm run lint",
    "install:all": "pnpm install && pnpm run install:frontend && pnpm run install:backend",
    "install:frontend": "cd apps/frontend && pnpm install",
    "install:backend": "cd apps/backend && pnpm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "monorepo",
    "react-native",
    "nestjs",
    "typescript",
    "english-learning"
  ],
  "author": "Catalpa65",
  "license": "MIT"
}